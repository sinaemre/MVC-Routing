@{
    ViewData["Title"] = "Detail";
}
@model DynamicVM

<div class="container">
    <div class="row">
        <div class="col-md-8">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Kişi Bilgisi</h3>
                </div>
                <div class="card-body">
                    <div>
                        <label asp-for="Id"></label>
                        : @Model.Id
                    </div>
                    <div>
                        <label asp-for="Name"></label>
                        : @Model.Name
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
